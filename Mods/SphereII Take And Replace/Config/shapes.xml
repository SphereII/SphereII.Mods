<configs>

	<!-- Let's pull off the boarded windows too, but use weak item instead.-->
	<append xpath="/shapes/shape[@name='windowBoarded']">
		<property name="Class" value="TakeAndReplace, SCore"/>
		<property name="CanPickup" value="true" param1="boardedWindowsSheet_weak"/>
		<property name="TakeDelay" value="8"/>
	</append>
	
</configs>